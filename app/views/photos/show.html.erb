<div id="user_info_header">
	<div id="user_info_header_left">
			<h1><%= @photo.name %></h1>
			Uploaded by <%=link_to @photo.owner.name, @photo.owner %>
	</div>
</div>

<div id="individual_photo">
	<%= image_tag get_photo(@photo, "original"), :size => "300x300" %>
	<div id="likes_and_comments">
		<div id="likes">
		</div>
		<div id="comments">
		</div>
	</div>
	<div id="for_clearing">
	</div>
</div>

